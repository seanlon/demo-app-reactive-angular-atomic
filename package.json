    "ng": "ng",
    "start": "npm run dev",
    "serve": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-dev": "cd build && node build-environment dev  &&  node build-mock-api && cd ..  ",
    "build-prod": "cd build && node build-environment prod  &&  node build-mock-api && cd .. && ng build --prod ",
    "dev": " concurrent \"npm run build-dev \" \"npm run serve \" \"json-server --port 3006 ./mock-server/mock-server-config-data.json --routes ./mock-server/mock-server-config-routes.json --middlewares ./mock-server/mock-server-middleware.js \"   ",
    "prod": "npm run build-prod   ",
    "doc": "compodoc -p src/tsconfig.app.json && compodoc -s",
    "validate": "npm run lint && npm run test"
  },
  "private": true,
